{
  "name": "react-basic-template",
  "version": "1.0.0",
  "description": "Starting point for a react frontend",
  "author": "Addison Moore",
  "license": "MIT",
  "scripts": {
    "dev": "parcel src/index.html",
    "clear": "rm -rf dist .parcel-cache build",
    "format": "prettier \"src/**/*.{js, html}\" --write",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --quiet",
    "start": "nodemon index.js",
    "build": "parcel build --no-cache src/index.html --dist-dir ./build/",
    "sass": "sass --watch sass:src/styles",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:update": "jest -u"
  },
  "dependencies": {
    "@emotion/react": "^11.1.5",
    "axios": "^0.21.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/plugin-transform-typescript": "^7.13.0",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@emotion/babel-preset-css-prop": "^11.2.0",
    "@emotion/eslint-plugin": "^11.2.0",
    "@parcel/transformer-typescript-tsc": "^2.0.0-alpha.3",
    "@testing-library/react": "^11.2.6",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-router": "^5.1.13",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "parcel": "^2.0.0-beta.2",
    "prettier": "^2.2.1",
    "typescript": "^4.2.3"
  }
}
