{
  "name": "react-basic-template",
  "version": "1.0.0",
  "description": "Starting point for a react frontend",
  "author": "Addison Moore",
  "license": "MIT",
  "scripts": {
    "dev": "parcel src/index.html",
    "clear": "rm -rf .cache/ dist/ .parcel-cache/ ./build",
    "format": "prettier \"src/**/*.{js, html}\" --write",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "start": "nodemon index.js",
    "build": "parcel build --no-cache src/index.html --dist-dir ./build/",
    "sass": "sass --watch sass:src/styles",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:update": "jest -u"
  },
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "axios": "^0.21.4",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/eslint-parser": "^7.15.4",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@babel/preset-env": "^7.15.4",
    "@babel/preset-react": "^7.14.5",
    "@emotion/babel-preset-css-prop": "^11.2.0",
    "@emotion/eslint-plugin": "^11.2.0",
    "@testing-library/react": "^12.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^27.1.1",
    "parcel": "^2.0.0-rc.0",
    "prettier": "^2.3.2"
  }
}
