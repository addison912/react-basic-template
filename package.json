{
  "name": "react-basic-template",
  "version": "1.0.0",
  "description": "Starting point for a react frontend",
  "author": "Addison Moore",
  "license": "MIT",
  "scripts": {
    "dev": "parcel src/index.html",
    "clear": "rm -rf .cache/ dist/ .parcel-cache/ ./build",
    "format": "prettier \"src/**/*.{js, html}\" --write",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "start": "nodemon index.js",
    "build": "parcel build --no-cache src/index.html --dist-dir ./build/",
    "sass": "sass --watch sass:src/styles"
  },
  "dependencies": {
    "@babel/plugin-transform-react-jsx": "^7.12.17",
    "@emotion/babel-preset-css-prop": "^11.2.0",
    "@emotion/react": "^11.1.5",
    "@reach/router": "^1.3.3",
    "axios": "^0.21.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "hash-source": "^1.0.5",
    "react": "^16.10.1",
    "react-dom": "^16.10.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "parcel": "^2.0.0-beta.1",
    "prettier": "^1.18.2"
  }
}
